# web/.dockerignore

# Ignore node_modules directory
node_modules/

# Ignore build output directory (we're building inside the container anyway)
dist/

# Ignore environment files that should not be committed/copied
.env
.env.*

# Ignore editor/IDE files
.vscode/
.idea/

# Ignore logs
npm-debug.log
yarn-debug.log
.pnpm-debug.log
